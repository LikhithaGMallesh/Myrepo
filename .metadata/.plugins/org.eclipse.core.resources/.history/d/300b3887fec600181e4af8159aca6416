package com.capgemini.insurance;

public abstract class Insurance implements CurrentAccount{
	
	String name;
	int policyId;
	double amount;
	
	public Insurance() {
		
		
	}
	
	public Insurance(String name, int policyId, double amount) {
		super();
		this.name = name;
		this.policyId = policyId;
		this.amount = amount;
	}

	
	
	public abstract void method1();
	
	
	@Override
	public String toString() {
		return "Insurance [name=" + name + ", policyId=" + policyId + ", amount=" + amount + "]";
	}
	
	
	

}
